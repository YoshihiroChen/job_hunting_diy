<!-- 交互优化：防止同一选项同时选为最喜欢和最不喜欢 -->
<script>
document.addEventListener("DOMContentLoaded", function() {
  for (let i = 1; i <= 32; i++) { // 这里5改成你的题目总数
    ["A", "B", "C", "D"].forEach(function(opt) {
      let most = document.getElementById(`q${i}_most_${opt}`);
      let least = document.getElementById(`q${i}_least_${opt}`);

      if (most && least) {
        most.addEventListener("change", function() {
          if (most.checked) {
            least.disabled = true;
          }
          ["A", "B", "C", "D"].forEach(function(o) {
            if (opt !== o) {
              let other = document.getElementById(`q${i}_least_${o}`);
              if (other) other.disabled = false;
            }
          });
        });
        least.addEventListener("change", function() {
          if (least.checked) {
            most.disabled = true;
          }
          ["A", "B", "C", "D"].forEach(function(o) {
            if (opt !== o) {
              let other = document.getElementById(`q${i}_most_${o}`);
              if (other) other.disabled = false;
            }
          });
        });
        most.addEventListener("click", function() {
          if (!most.checked) {
            least.disabled = false;
          }
        });
        least.addEventListener("click", function() {
          if (!least.checked) {
            most.disabled = false;
          }
        });
      }
    });
  }
});
</script>
